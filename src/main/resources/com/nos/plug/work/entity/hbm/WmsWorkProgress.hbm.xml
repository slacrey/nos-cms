<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.nos.plug.work.entity">
    <class
            name="com.nos.plug.work.entity.WmsWorkProgress"
            table="wms_work_progress"
            >
        <meta attribute="sync-DAO">false</meta>
        <id
                name="id"
                type="long"
                column="id"
                >
            <generator class="identity"/>
        </id>

        <property name="addDate" column="add_date" type="timestamp" not-null="false" length="19"/>
        <property name="progressImg" column="progress_img" type="string" not-null="false" length="255"/>
        <property name="content" column="content" type="string" not-null="false" length="255"/>
        <property name="fkWorkId" column="fk_work_id" type="long" not-null="false" length="100"/>
        <property
                name="remarks"
                column="remarks"
                type="string"
                not-null="false"
                length="255"
                />
        <property name="createBy" column="create_by" type="long" not-null="false" length="100"/>
        <property name="createDate" column="create_date" type="timestamp" not-null="true" length="19"/>
        <property name="updateBy" column="update_by" type="long" not-null="false" length="100"/>
        <property name="updateDate" column="update_date" type="timestamp" not-null="true" length="19"/>
        <property name="delFlag" column="del_flag" type="string" not-null="false" length="1"/>


    </class>
</hibernate-mapping>